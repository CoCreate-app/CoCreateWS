    "@cocreate/industry": "^1.17.2",
    "@cocreate/metrics-server": "^1.9.7",
    "@cocreate/mongodb": "^1.10.6",
    "@cocreate/notification": "^1.2.1",
    "@cocreate/organizations": "^1.23.2",
